{
  "compilerOptions": {
    "outDir": "./.tsbuild/",
    "module": "commonjs",
    "target": "es2018",
    "types": [
        "node",
        "cypress"
    ],
    "moduleResolution": "node",
    "checkJs": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts", "**/*.json"],
  "exclude": ["node_modules"]
}
